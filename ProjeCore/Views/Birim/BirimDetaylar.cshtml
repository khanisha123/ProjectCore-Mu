@using ProjeCore.Models
@model List<Personel>
@{
    ViewData["Title"] = "BirimDetaylar";
}
<h1>Birim Ismi :</h1> <br /> 
<h3 style="color:cyan;">
    @ViewBag.d
</h3>
<br />
<br />
<table class="table table-active">
    <tr>
        <th>Id</th>
        <th>Ad</th>
        <th>Soyad</th>
        <th>Şehir</th>

    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.PersonelId</td>
            <td>@item.Ad</td>
            <td>@item.Soyad</td>
            <td>@item.Şehir</td>


        </tr>
    }

</table>
<a asp-controller="Birim" asp-action="Index" class="btn btn-info">Go to Back</a>


